{
  "name": "wearesho-team/bobra-cpa",
  "description": "CPA Networks integration",
  "type": "library",
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.3",
    "horat1us/yii2-base": "^1.13.1",
    "horat1us/yii2-environment-config": "^1.0",
    "yiisoft/yii2": "^2.0.14.2",
    "yiisoft/yii2-queue": "^2.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Horat1us/yii2-phpunit.git"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.3.2",
    "vlucas/phpdotenv": "^2.5",
    "yiisoft/yii2-phpunit": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Bobra\\Cpa\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Bobra\\Cpa\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
